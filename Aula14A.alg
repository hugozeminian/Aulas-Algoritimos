algoritmo "TesteVetor"
// Função :
// Autor :
// Data : 11/22/2020
// Seção de Declarações 
var
   V: vetor[1..6] de inteiro
   C: Inteiro

inicio
// Seção de Comandos
   Para C <- 1 ate 6 faca
        Escreva("Digite o ", c, "o, valor:" )
        Leia(v[c])
   FimPara
   
   Para C <- 1 ate 6 faca
        Escreva("{", v[C], "}")
   FimPara



fimalgoritmo
