algoritmo "Creuza01"
// Função :
// Autor :
// Data : 11/14/2020
// Seção de Declarações
var
   ano_atual, ano_nasc, idade: inteiro

inicio
// Seção de Comandos
         Escreva("Qual o ano atual?")
         Leia (ano_atual)
         Escreva("Em que ano eu nasci?")
         Leia (ano_nasc)
         idade <- ano_atual - ano_nasc
         Escreva ("Minha idade será", idade)

fimalgoritmo
