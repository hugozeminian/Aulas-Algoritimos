algoritmo "Conta1a10"
// Função :
// Autor :
// Data : 11/21/2020
// Seção de Declarações 
var
   Cont, N, R: Inteiro

inicio
// Seção de Comandos 
   Cont <- 1
   Escreval("quer ver a tabuada de qual numero?")
   Leia(N)
   Repita
   R <- N * cont
         EscrevaL(N, " x ", Cont, " = ", R)
         Cont <- Cont + 1
   Ate (cont > 10)


fimalgoritmo
