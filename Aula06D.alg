algoritmo "Creuza04"
// Função :
// Autor :
// Data : 11/14/2020
// Seção de Declarações
var
    preco, imposto: Real

inicio
// Seção de Comandos

         Escreva("Qual o preço do produto? US$")
         Leia(preco)
         imposto <- preco * 60 / 100
         Escreva ("O imposto será de US$: ", imposto)



fimalgoritmo
